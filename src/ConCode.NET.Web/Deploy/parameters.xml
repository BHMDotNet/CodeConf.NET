<?xml version="1.0" encoding="utf-8" ?>
<parameters>
  <parameter name="ConnectionString-ConCode"
     defaultValue="Parameterized connection string!">
    <parameterEntry
       type="TextFile"
       scope="appsettings.json"
       match="(?&lt;=&quot;ConCode&quot;:\s*&quot;)[^&quot;]*"/>
  </parameter>
  <parameter name="SiteName">
    <parameterEntry
       type="providerPath"
       scope="IisApp"
       match="ConCode.NET"/>
    <parameterEntry
       type="providerPath"
       scope="contentPath"
       match="ConCode.NET"/>
    <parameterEntry
       type="providerPath"
       scope="createApp"
       match="ConCode.NET"/>
  </parameter>
</parameters>